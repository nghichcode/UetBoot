{
	"name":"__MSG_appName__",
	"version":"1.2",
	"manifest_version":2,

	"description":"__MSG_appDescription__",
	"default_locale":"en",
	"icons":{
		"32":"images/u3.png",
		"48":"images/u2.png",
		"128":"images/u1.png"
	},

	"browser_action":{
		"default_popup": "page/page.html",
		"default_icon": {
			"32":"images/u3.png",
			"48":"images/u2.png",
			"128":"images/u1.png"
		}
	},

	"content_scripts": [{
		"matches": ["http://courses.uet.vnu.edu.vn/*","https://courses.uet.vnu.edu.vn/*"],"js": ["ScriptOnLoad.js"]
	}],

	"options_page":"options/options.html",
	"permissions":["http://*/*","https://*/*","storage"]
	// "permissions":["http://courses.uet.vnu.edu.vn/*","https://courses.uet.vnu.edu.vn/*","http://localhost/*"]
}